{
  "name": "gainstrack",
  "displayName": "GainsTrack",
  "version": "0.1.0",
  "description": "Workout tracker app",
  "homepage": "https://github.com/dtom90/GainsTrack",
  "repository": "https://github.com/dtom90/GainsTrack",
  "author": "David Thomason",
  "license": "MIT",
  "private": true,
  "framework7": {
    "cwd": "/Users/david/Development/GainsTrack",
    "type": [
      "cordova"
    ],
    "name": "GainsTrack",
    "pkg": "app.gainstrack",
    "framework": "vue",
    "template": "single-view",
    "cssPreProcessor": "scss",
    "bundler": "webpack",
    "cordova": {
      "folder": "cordova",
      "platforms": [
        "ios",
        "android"
      ],
      "plugins": [
        "cordova-plugin-statusbar",
        "cordova-plugin-keyboard",
        "cordova-plugin-splashscreen",
        "cordova-plugin-wkwebview-engine"
      ]
    },
    "webpack": {
      "developmentSourceMap": true,
      "productionSourceMap": true,
      "hashAssets": false,
      "preserveAssetsPaths": false,
      "inlineAssets": true
    },
    "theming": {
      "customColor": true,
      "color": "#0A1344",
      "darkTheme": false,
      "iconFonts": true,
      "fillBars": false
    },
    "customBuild": false
  },
  "scripts": {
    "web:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js",
    "web:dev:test": "cross-env LOAD_SAMPLE_STATE=true yarn run web:dev",
    "build:dev": "cross-env NODE_ENV=development node ./build/build.js",
    "build:prod": "cross-env NODE_ENV=production node ./build/build.js",
    "build:test": "LOAD_SAMPLE_STATE=true yarn run build:prod",
    "serve": "node server.js & wait-on http://localhost:8080",
    "ios:lr": "cd cordova && cordova run ios -- --lr",
    "ios:lr:test": "cross-env LOAD_SAMPLE_STATE=true yarn run ios:lr",
    "ios:build": "cd cordova && cordova build ios",
    "ios:run": "cd cordova && cordova run ios",
    "ios:run:dev": "cross-env TARGET=cordova yarn run build:dev && yarn run ios:run",
    "ios:run:prod": "cross-env TARGET=cordova yarn run build:prod && yarn run ios:run",
    "ios:run:test": "cross-env TARGET=cordova yarn run build:test && yarn run ios:run",
    "ios:build:prod": "cross-env TARGET=cordova yarn run build:prod && yarn run ios:build",
    "open-xcode": "open ./cordova/platforms/ios/GainsTrack.xcworkspace",
    "cordova:build:dev": "cross-env TARGET=cordova yarn run build:dev && cd cordova && cordova build",
    "cordova:build:prod": "cross-env TARGET=cordova yarn run build:prod && cd cordova && cordova build",
    "android:build:dev": "cross-env TARGET=cordova yarn run build:dev && cd cordova && cordova build android",
    "android:build:prod": "cross-env TARGET=cordova yarn run build:prod && cd cordova && cordova build android",
    "postinstall": "cpy './node_modules/framework7-icons/fonts/*.*' './src/fonts/'",
    "audit:packages": "yarn audit || test $? -eq 0",
    "lint": "./node_modules/.bin/eslint --no-fix --ext .js,.vue src tests",
    "test:e2e": "cypress run",
    "test:basic": "yarn audit:packages && yarn lint && yarn build:test",
    "test": "yarn audit:packages && yarn lint && yarn build:test && yarn serve && yarn test:e2e"
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "dom7": "^2.1.3",
    "fastclick": "^1.0.6",
    "framework7": "^5.7.10",
    "framework7-icons": "^3.0.0",
    "framework7-vue": "^5.7.10",
    "template7": "^1.4.2",
    "vue": "^2.6.10",
    "vuex": "^3.1.0",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-jsx": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.7.6",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.3",
    "copy-webpack-plugin": "^5.1.0",
    "cpy-cli": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.0",
    "cypress": "^4.12.1",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^2.2.2",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "acorn": ">=6.4.1",
    "dot-prop": ">=5.1.1",
    "elliptic": ">=6.5.3",
    "http-proxy": ">=1.18.1",
    "kind-of": ">=6.0.3",
    "lodash": ">=4.17.19",
    "minimist": ">=1.2.3",
    "serialize-javascript": ">=3.1.0",
    "tree-kill": ">=1.2.2",
    "websocket-extensions": ">= 0.1.4",
    "yargs-parser": ">=13.1.2 <14.0.0 || >=15.0.1 <16.0.0 || >=18.1.2"
  }
}
